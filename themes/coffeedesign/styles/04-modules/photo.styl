.photo
  display inline-block
  width 100%
  background-size cover
  background-position center
  background-repeat no-repeat

  &:after
    content ""
    display block
    padding-top 100%

  img 
    display none

  &.sub-circle
    border-radius 500em
    border 4px solid rgba(black, 0.1)
    background-origin border-box

  &.sub-contain
    background-size contain